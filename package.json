{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "postinstall": "husky install",
    "test": "jest",
    "storybook": "storybook dev -s ./public -p 6006",
